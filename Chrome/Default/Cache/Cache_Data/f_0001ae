inity :random-colors="[&quot;#fefcee&quot;,&quot;#faf7ec&quot;,&quot;#f9f9f3&quot;,&quot;#f8f9ec&quot;,&quot;#fffdea&quot;,&quot;#faf9fb&quot;,&quot;#faf5f2&quot;,&quot;#f7f4f2&quot;]" :perfume-id="24785" sentiment="all"></reviews-infinity>
